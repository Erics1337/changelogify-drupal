    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/changelogify",
    "support": {
        "issues": "https://www.drupal.org/project/issues/changelogify"
    },
    "require": {
        "drupal/core": "^10.3 || ^11"
    },
    "require-dev": {
        "drupal/core-dev": "^10.3 || ^11"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\changelogify\\": "src/"
        }
    }
}
